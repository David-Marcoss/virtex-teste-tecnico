generator client {
  provider = "prisma-client"
  output = "../prisma/generated"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum StateEnum {
  OFFLINE
  ONLINE
}


enum OltTypeEnum {
  HUAWEI
  ZTE
  ZTE_STATE
}

model OltsInfo {
  id           String   @id @default(cuid())
  slot         String
  port         String
  ont_id       String
  sn           String? 
  state        StateEnum?
  oltType      OltTypeEnum
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}